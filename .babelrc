{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy",
    [
      "flow-runtime",
      {
        "assert": true,
        "annotate": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": []
    }
  }
}
